{
  "inbounds": [
    {
      "port": {{ app_port }},
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "{{ app_uuid }}",
            "alterId": 0
          }
        ]
      },
      "streamSettings": {
        "network":"ws",
        "wsSettings": {
          "path": "/websocket"
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    }
  ]
}
